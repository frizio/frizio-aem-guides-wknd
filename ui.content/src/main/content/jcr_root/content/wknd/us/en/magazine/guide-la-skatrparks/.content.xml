                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wknd/components/text"
                        text="&lt;blockquote>&amp;quot;There is no better place to shred then Los Angeles.”.&lt;/blockquote>&#xd;&#xa;&lt;p>- Jacob Waters - Pro Stakers&lt;/p>&#xd;&#xa;"
                        textIsRich="true"/>
